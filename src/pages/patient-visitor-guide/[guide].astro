---
import GuidePage from "../../components/patient-visitor-page/guide-page/guide-page";
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
  return [
    { params: { guide: "preparing-for-your-stay" } },
    { params: { guide: "during-your-stay" } },
    { params: { guide: "discharge-and-follow-up" } },
    { params: { guide: "getting-help-at-home" } },
    { params: { guide: "discharge-to-a-facility" } },
    { params: { guide: "visitation" } },
  ];
}

const { guide } = Astro.params;
---

<Layout>
  <GuidePage title={guide} pathname={`/patient & visitor guide/${guide}`} />
</Layout>
