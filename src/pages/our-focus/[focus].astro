---
import FocusPage from "../../components/our-focus/focus/focus";
import Layout from "../../layouts/Layout.astro";

export function getStaticPaths() {
  return [
    { params: { focus: "psychiatric-inpatient-treatment-program" } },
    { params: { focus: "conditions-and-therapy-programs" } },
    { params: { focus: "nutrition-program" } },
    { params: { focus: "brochure" } },
    { params: { focus: "referral-process" } },
  ];
}

const { focus } = Astro.params;
---

<Layout>
  <FocusPage title={focus} pathname={`/our-focus/${focus}`} />
</Layout>
